{
  "name": "gamestore",
  "version": "1.0.0",
  "description": "Game store",
  "main": "dist/main.js",
  "scripts": {
    "start": "cross-env NODE_ENV=prod node ./dist/index.js",
    "start:dev": "tsc-watch --project . --onSuccess \"cross-env NODE_ENV=dev node ./dist/index.js\"",
    "format": "prettier --write \"src/**/*\"",
    "lint": "eslint -c .eslintrc.js --ext .ts ./src",
    "build": "npm run build:clean && tsc",
    "build:clean": "rimraf dist",
    "prepare": "husky install",
    "new:module": "hygen new module"
  },
  "repository": "https://github.com/terminaate/game-store",
  "keywords": [],
  "author": "Terminaate",
  "license": "MIT",
  "devDependencies": {
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.16",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.33.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "husky": "^8.0.3",
    "hygen": "^6.2.11",
    "prettier": "^2.8.3",
    "rimraf": "^4.1.2",
    "tsc-watch": "^6.0.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "class-transformer": "^0.5.1",
    "class-transformer-validator": "^0.9.1",
    "class-validator": "^0.14.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^6.9.0"
  }
}
